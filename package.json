{
  "name": "1.1-cli",
  "version": "0.0.1",
  "description": "",
  "author": "Jeusto",
  "type": "module",
  "bin": {
    "1.1-cli": "./dist/index.js"
  },
  "scripts": {
    "start": "npm run build-ts && node dist/index.js",
    "dev": "npm run build-ts && node --inspect-brk dist/index.js",
    "build": "npm run type-check && npm run build-ts",
    "build-ts": "rm -Rf dist/ && tsc --build tsconfig.json",
    "type-check": "tsc --noEmit && eslint .",
    "test": "npm run build && jest --verbose",
    "bundle": "npm run build && pkg . --out-dir ./exec/"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.14.6",
    "@types/qrcode": "^1.5.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.2",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.2.0",
    "jest-watch-typeahead": "^2.2.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "inquirer": "^9.1.4",
    "node-fetch": "^3.3.0",
    "ora": "^6.1.2",
    "qrcode": "^1.5.1"
  },
  "keywords": [
    "1pt.one",
    "url shortener",
    "cli",
    "typescript",
    "nodejs",
    "node",
    "commander",
    "inquirer",
    "qrcode",
    "node-fetch",
    "ora",
    "chalk"
  ]
}
